{
  "manifest_version": 3,
  "name": "LDR Sync",
  "description": "An extention to control the playback of netflix",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "*://*.netflix.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.netflix.com/*"],
      "js": ["content.js"]
    }
  ]
}

